#ifndef OSKAR_TELESCOPE_EDITOR_CONFIG_VERSION_H
#define OSKAR_TELESCOPE_EDITOR_CONFIG_VERSION_H

#define BUILD_TYPE "@CMAKE_BUILD_TYPE@"
#define PROJECT_NAME "@PROJECT_NAME@"
#define VERSION "@PROJECT_VERSION@"
#if defined(__DATE__) && defined(__TIME__)
#   define VERSION_LONG "@PROJECT_VERSION@ (@CMAKE_BUILD_TYPE@), " __DATE__ " " __TIME__
#   define BUILD_INFO "(@CMAKE_BUILD_TYPE@) v" VERSION " (" __DATE__ " " __TIME__")"
#   define NAME PROJECT_NAME " (@CMAKE_BUILD_TYPE@) " VERSION " (" __DATE__ " " __TIME__")"
#else
#   define VERSION_LONG "@PROJECT_VERSION@ (@CMAKE_BUILD_TYPE@)"
#   define BUILD_INFO "(@CMAKE_BUILD_TYPE@) v" VERSION
#   define NAME PROJECT_NAME " (@CMAKE_BUILD_TYPE@) " VERSION
#endif

#endif // OSKAR_TELESCOPE_EDITOR_CONFIG_VERSION_H
